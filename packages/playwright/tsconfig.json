{
  "extends": "../../tsconfig.base",
  "include": [
    "tests",
    "playwright.config.ts",
    "../snaplet/src",
    "../app",
    "../wagmi",
    "../contracts/out",
    "../webauthn-authenticator",
    "../../supabase"
  ],
  "compilerOptions": {
    "noEmit": true,
    "composite": true,
    "target": "ESNext",
