      "@0xsend/webauthn-authenticator": ["../webauthn-authenticator/src"],
      "@0xsend/webauthn-authenticator/*": ["../webauthn-authenticator/src/*"],
      "@my/supabase": ["../../supabase"],
      "@my/supabase/*": ["../../supabase/*"]
    }
  },
  "references": []
