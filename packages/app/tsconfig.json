      "@my/contracts/out/*": ["../contracts/out/*"],
      "@my/supabase": ["../../supabase"],
      "@my/supabase/*": ["../../supabase/*"],
      "@daimo/expo-passkeys/*": ["../daimo-expo-passkeys/src/*"]
