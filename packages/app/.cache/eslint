n    // No-op on web - pageviews are captured automatically via capture_pageview option\n  },\n\n  reset() {\n    if (!initialized) return\n    posthog.reset()\n  },\n\n  isInitialized() {\n    return initialized\n  },\n\n  captureException(error: unknown, properties?: ExceptionProperties) {\n    if (!initialized) {\n      log('Not initialized, cannot capture exception')\n      return\n    }\n\n    posthog.captureException(error, {\n      source: properties?.source,\n      handled: properties?.handled ?? true,\n      ...properties?.context,\n    })\n  },\n}\n","/Users/erickho/send_org/sendapp/packages/app/analytics/index.native.ts",[],[],"/Users/erickho/send_org/sendapp/packages/app/analytics/index.ts",[],[],"/Users/erickho/send_org/sendapp/packages/app/analytics/types.ts",[],[],"/Users/erickho/send_org/sendapp/packages/app/components/ActivityRowLayout.tsx",[],[],"/Users/erickho/send_org/sendapp/packages/app/components/AvatarMenuButton/AvatarMenuButton.android.tsx",[],[],"/Users/erickho/send_org/sendapp/packages/app/components/AvatarMenuButton/AvatarMenuButton.ios.tsx",[],[],"/Users/erickho/send_org/sendapp/packages/app/components/AvatarMenuButton/AvatarMenuButton.tsx",[],[],"/Users/erickho/send_org/sendapp/packages/app/components/BottomTabBar/BottomNavBar.native.tsx",[],[],"/Users/erickho/send_org/sendapp/packages/app/components/BottomTabBar/BottomNavBar.tsx",[],[],"/Users/erickho/send_org/sendapp/packages/app/components/BottomTabBar/BottomNavBarContent.tsx",[],[],"/Users/erickho/send_org/sendapp/packages/app/components/BottomTabBar/BottomNavBarWrapper.tsx",[],[],"/Users/erickho/send_org/sendapp/packages/app/components/BottomTabBar/useTabBarSize.ts",[],[],"/Users/erickho/send_org/sendapp/packages/app/components/CoinsModal.tsx",[],[],"/Users/erickho/send_org/sendapp/packages/app/components/ComingSoon.tsx",[],[],"/Users/erickho/send_org/sendapp/packages/app/components/ErrorBoundary/AnalyticsErrorBoundary.tsx",[],[],"/Users/erickho/send_org/sendapp/packages/app/compon