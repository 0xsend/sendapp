{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "noEmit": true,
    "composite": true,
    "moduleResolution": "bundler",
    "baseUrl": "./src",
    "paths": {
      "@my/supabase/*": ["../../supabase/*"],
      "app/*": ["../app/*"],
      "@my/wagmi": ["../wagmi/src"],
      "@my/wagmi/*": ["../wagmi/src/*"],
      "@my/workflows": ["./src"],
      "@my/workflows/*": ["./src/*"],
      "@my/temporal": ["../temporal/src"],
      "@my/temporal/*": ["../temporal/src/*"]
    }
  },
  "include": [
    "tsconfig.json",
    "./src",
    "../temporal/src",
    "./jest.config.ts",
    "../../supabase",
    "../app",
    "../wagmi/src",
    "../../globals.d.ts",
    "../../environment.d.ts"
  ]
}
