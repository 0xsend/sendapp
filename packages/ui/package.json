    "@tamagui/animations-react-native": "^1.135.4",
    "@tamagui/cli": "^1.135.4",
    "@tamagui/config": "^1.135.4",
    "@tamagui/create-theme": "^1.135.4",
    "@tamagui/helpers-icon": "^1.135.4",
    "@tamagui/react-native-media-driver": "^1.135.4",
    "@tamagui/shorthands": "^1.135.4",
    "@tamagui/theme-builder": "^1.135.4",
    "@tamagui/themes": "^1.135.4",
    "@tamagui/toast": "^1.135.4",
