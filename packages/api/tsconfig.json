{
  "extends": "../../tsconfig.base",
  "include": [
    "src",
    "../app",
    "../wagmi/src",
    "../ui/src",
    "../../supabase",
    "../workflows/src",
    "../temporal/src"
  ],
  "compilerOptions": {
    "noEmit": true,
    "composite": true,
    "baseUrl": ".",
    "moduleResolution": "bundler",
    "paths": {
      "app/*": ["../app/*"],
      "@my/wagmi": ["../wagmi/src"],
      "@my/wagmi/*": ["../wagmi/src/*"],
      "@my/supabase/*": ["../../supabase/*"],
      "@my/workflows": ["../workflows/src/all-workflows.ts"],
      "@my/workflows/*": ["../workflows/src/*"],
      "@my/temporal": ["../temporal/src"],
      "@my/temporal/*": ["../temporal/src/*"]
    }
  },
  "references": []
}
