{
  "cli": {
    "version": ">= 10.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "environment": "production",
      "channel": "development",
      "env": {
        "APP_VARIANT": "production"
      }
    },
    "preview": {
      "distribution": "internal",
      "environment": "production",
      "channel": "preview",
      "env": {
        "APP_VARIANT": "preview"
      }
    },
    "staging": {
      "distribution": "internal",
      "environment": "production",
      "channel": "staging",
      "env": {
        "APP_VARIANT": "staging"
      }
    },
    "production": {
      "environment": "production",
      "autoIncrement": true,
      "channel": "production",
      "env": {
        "APP_VARIANT": "production"
      }
    }
  }
}
