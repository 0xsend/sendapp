{
  "type": "module",
  "name": "distributor",
  "description": "This is a singleton Fastify app that analyzes Ethereum mainnet blocks for Send token transfers and re-calculates the distribution shares for each Send token holder.",
  "private": true,
  "version": "0.0.1",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "yarn _with-env yarn vitest",
    "_with-env": "dotenv -e ../../.env -c -- ",
    "start": " yarn _with-env bun --bun run ./src/server.ts",
    "dev": "yarn _with-env bun --bun run --watch ./src/server.ts"
  },
  "keywords": [],
  "author": "BigBoss <bigboss@metalrodeo.xyz>",
  "license": "ISC",
  "dependencies": {
    "@my/supabase": "workspace:*",
    "@my/wagmi": "workspace:*",
    "@supabase/supabase-js": "^2.38.5",
    "@wagmi/core": "^1.4.12",
    "app": "workspace:*",
    "express": "^4.18.2",
    "lru-cache": "^10.0.1",
    "pino": "^8.16.1",
    "viem": "^1.21.1"
  },
  "devDependencies": {
    "@types/express": "^4",
    "@types/node": "^18.6.4",
    "@types/supertest": "^2.0.16",
    "bun-types": "latest",
    "debug": "^4.3.4",
    "dotenv-cli": "^6.0.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "vitest": "^0.34.6"
  }
