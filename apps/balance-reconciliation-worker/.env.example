# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# RPC Configuration
RECONCILIATION_RPC_URL=https://mainnet.base.org

# Worker Configuration
RECONCILIATION_BATCH_SIZE=100              # Number of balances to process per loop
RECONCILIATION_RATE_LIMIT_MS=100           # Delay between RPC calls (ms)
RECONCILIATION_POLL_INTERVAL_MS=60000      # How often to run reconciliation loop (ms)
RECONCILIATION_CHAIN_ID=8453               # Base mainnet

# Logging
LOG_LEVEL=info
